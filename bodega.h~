#include <iostream>
#include <string>
#include <vector>

using namespace std;

#ifndef BODEGA_H
#define BODEGA_H
class bodega{
	protected:
		vector<ingredientes*> ingredientes;
		int cantidad;
	public:
		bodega(vector<ingredientes*>,int);
		bodega();
		vector<bodega*> getIngredientes();
		void setIngredientes(ingredientes*);

		int getCantidad();
		void setCantidad(int);

};
#endif